{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/generate-sw.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpeg|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "profile",
      "urls": [
        "/profile/"
      ],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "7d",
        "strategy": "freshness"
      }
    },
    {
      "name": "sounds",
      "urls": [
        "/sounds/*/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "3d",
        "strategy": "freshness"
      }
    },
    {
      "name": "artists",
      "urls": [
        "/users/**"
      ],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "1d",
        "strategy": "performance"
      }
    },
    {
      "name": "sounds-list",
      "urls": [
        "/sounds/",
        "/albums/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1d",
        "strategy": "performance"
      }
    },
    {
      "name": "styles",
      "urls": [
        "/styles/**"
      ],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "3d",
        "strategy": "performance"
      }
    }
  ]
}